<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="593.0" prefWidth="690.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxgui.StudentView">
   <children>
      <ListView fx:id="coursesListView" layoutX="31.0" layoutY="133.0" prefHeight="273.0" prefWidth="231.0">
         <tooltip>
            <Tooltip fx:id="summaryTooltip" />
         </tooltip></ListView>
      <Button fx:id="listCoursesButton" layoutX="89.0" layoutY="415.0" mnemonicParsing="false" onAction="#displayClasses" text="List courses" />
      <Button fx:id="btnEnroll" layoutX="29.0" layoutY="478.0" mnemonicParsing="false" onAction="#enroll" text="Zapisz sie" />
      <Button fx:id="btnWithdraw" layoutX="154.0" layoutY="478.0" mnemonicParsing="false" onAction="#withdraw" text="Wypisz sie" />
      <TableView fx:id="enrolledCoursesTable" layoutX="377.0" layoutY="133.0" prefHeight="273.0" prefWidth="284.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Nazwa" />
          <TableColumn fx:id="gradesColumn" prefWidth="75.0" text="Oceny" />
            <TableColumn fx:id="averageColumn" prefWidth="75.0" text="Åšrednia" />
        </columns>
      </TableView>
      <Label layoutX="81.0" layoutY="91.0" text="Dostepne zajecia" />
      <Label layoutX="418.0" layoutY="91.0" text="Zajecia na ktorych jestes zapisany" />
      <Button fx:id="refreshTableView" layoutX="488.0" layoutY="415.0" mnemonicParsing="false" onAction="#refreshTable" text="Refresh" />
      <Button fx:id="backBtn" layoutX="29.0" layoutY="536.0" mnemonicParsing="false" onAction="#changeScene" text="Wroc" />
   </children>
</AnchorPane>
